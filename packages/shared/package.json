{
  "name": "nest-devtools-shared",
  "version": "0.1.0",
  "description": "Tipos TypeScript compartilhados para Nest DevTools Telescope",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "nestjs",
    "devtools",
    "typescript",
    "types",
    "telescope",
    "shared-types"
  ],
  "author": "Lucas",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucasbrito-wdt/nest-devtools-agent.git",
    "directory": "packages/shared"
  },
  "bugs": {
    "url": "https://github.com/lucasbrito-wdt/nest-devtools-agent/issues"
  },
  "homepage": "https://github.com/lucasbrito-wdt/nest-devtools-agent#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
